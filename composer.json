{
    "name": "vulnerable/dummy-traversal",
    "description": "PHPUnit and Infection testing for path traversal vulnerabilities.",
    "type": "project",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "infection/infection": "0.31",
        "infection/mutator": "0.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "infection": "infection",
        "mutfix": "php bin/generate-tests-from-mutants.php --log=build/infection-log.json --prompt=prompts/mutant_fix_prompt.md --model=gpt-4o"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
